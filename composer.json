{
    "name": "amptech/laravel-solid-structure",
    "description": "Generador de arquitectura SOLID para Laravel (Repository Pattern, Services, DTOs)",
    "type": "library",
    "version": "1.0.0",
    "homepage": "https://github.com/oamperezp/laravel-solid-structure",
    "license": "MIT",
    "keywords": ["laravel", "solid", "repository", "service", "architecture", "dto", "pattern"],
    "authors": [
        {
            "name": "Oscar Amperez",
            "email": "oamperezp@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3"
    },
    "autoload": {
        "psr-4": {
            "AmpTech\\LaravelSolidStructure\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmpTech\\LaravelSolidStructure\\SolidStructureServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/oamperezp/laravel-solid-structure/issues",
        "source": "https://github.com/oamperezp/laravel-solid-structure"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}